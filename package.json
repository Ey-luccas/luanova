  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:web\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:web": "cd web && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../web && npm install",
    "build": "npm run build:backend && npm run build:web",
    "build:backend": "cd backend && npm run build",
    "build:web": "cd web && npm run build",
    "start": "concurrently \"npm:start:backend\" \"npm:start:web\"",
    "start:backend": "cd backend && npm start",
    "start:web": "cd web && npm start"
  },
